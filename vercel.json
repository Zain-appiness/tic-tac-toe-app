{
    "cleanUrls": true,  
  "trailingSlash": false,  
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build", 
          "outputDirectory": "build"
        }
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]

  }
  